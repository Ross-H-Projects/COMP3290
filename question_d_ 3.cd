constants
	COUNT = 2
types
	/-- declare struct
	coord is
		x : real,
		y : real
	end 

	/-- declare and init array
	/-- every element of coords's x and y vars are 
	/-- initialised to 0.0
	coords is array [COUNT] of coord
arrays
	xyCoords : coords 
main
	/-- declare primitives to be used
	/-- in main pogram
	x : integer,
	z : integer,
	i : real
begin
	i = 1.0;

	/-- set array elements
	xyCoords[0].x = i;
	xyCoords[0].y = 2.0;
	
	xyCoords[1].x = 1.0;
	xyCoords[1].y = 2.0;
	
	incrementElementsOfArray (xyCoords);

	x = 1; 
	z = doubleNum (x);
end 
