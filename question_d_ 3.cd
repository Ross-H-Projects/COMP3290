/-- pass array by reference, pass length by value
func incrementElementsOfArray (passArray : coords, length: integer) : void
tmp : real,
i	: integer,
k	: integer
begin
	for(i = 0, k = length; i < k)
		tmp = coords[i].x + 1.0;
		coords[i].x = tmp;
		
		tmp = coords[i].y + 1.0;
		coords[i].y = tmp;
	end
	return;
end

/-- pass a by value
func doubleNum (a : integer) : integer
tmp: integer
begin
	tmp = a * 2;
	return tmp;
end